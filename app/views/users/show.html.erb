<% provide(:title, @user.name) %>
<!--MAP-->
<div class="row">
    <div class ="map">
        <div id="map" class = "col-md-12"></div>
    </div>
</div>


<!--HIDDEN INPUT. USES DATA FROM GEOJSON TO CREATE GOOGLE EVENT-->
<input id="title" type="text" style="display:none;"/>
<input id="description" type="text" style="display:none;"/>


<div class="row ribbons">
    <!--DIRECTIONS INTRO RIBBON-->
    <div class= "col-md-12 intro">
        <i class="fa fa-road fa-inverse fa-lg"></i> Click a street.
        <i class='fa fa-bell-o fa-lg'></i> Set a reminder.
    </div>
    
    <!--SET ALARM OPTIONS-->
    <div class= "col-md-12" id="setTime">
        <button class="btn setAlert" id="setTime1"></button>
        <button class="btn setAlert" id="setTime2"></button>
    </div>
    
    <!--CONFIRMATION OF GOOGLE EVENT CREATION RIBBON-->
    <div class= "col-md-12 setTime">
        <i class= "fa fa-calendar fa-lg"></i> Google reminder set.
    </div>
</div>

<script src="https://apis.google.com/js/client.js?onload=checkAuth"></script>







         

